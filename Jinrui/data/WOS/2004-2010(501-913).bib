@inproceedings{ ISI:000242339000056,
Author = {Jasinska, Emilia and Hallett, Paul and Horn, Rainer},
Editor = {{Horn, R and Fleige, H and Peth, S and Peng, XH}},
Title = {{Spatial distribution of hydrophobic and hydrophilic compounds and
   repellency in aggregates}},
Booktitle = {{SOIL MANAGEMENT FOR SUSTAINABILITY}},
Series = {{Advances in Geoecology}},
Year = {{2006}},
Volume = {{38}},
Pages = {{467+}},
Note = {{17th Conference of the International-Soil-Tillage-Research-Organisation,
   Christian Albrechts Univ, Kiel, GERMANY, AUG 28-SEP 02, 2006}},
Organization = {{Int Soil Tillage Res Org; Inst Plant Nutr \& Soil Sci; Minist Agr,
   Environm \& Rural Landscape}},
Abstract = {{Water repellency in soils has a great potential for stabilizing soil
   structure and protecting organic matter against mineralization. The
   heterogenic distribution of water repellency could be a consequence of
   different amounts of organic compounds present at various scales. The
   objectives of this study were to identify the spatial distributions of
   hydrophobic and hydrophilic carbon groups at different aggregate scales
   in relation to their water repellencies. Measurements were conducted on
   both homogenized soil and soil aggregates from silty loam Anthrosol and
   Luvisol under pasture and conservation and conventional tillage sampled
   from three soil horizons. The aggregates were fractionated into
   different size classes by dry sieving; single aggregates were separated
   into external, transitional and interior regions using the newly
   developed soil aggregate erosion chamber. Materials from different
   aggregate size classes or aggregate layer were used for carbon and
   nitrogen content measurements and for DRIFT spectrometric analyzes of
   functional hydrophobic and hydrophilic groups. Water repellency was
   identified on the surfaces of different size class aggregates,
   quantified using the sorptivity method.
   The results showed that outer surfaces of aggregates from no tilled
   soils had the greatest carbon contents and the greatest intensity for
   absorption of hydrophobic C-H and hydrophilic C=O groups. Differences in
   subcritical water repellencies for all soil aggregates were found
   between aggregate size classes which were affected by the type of
   vegetation and soil depth. Very poor correlations were observed between
   repellency in soil aggregates and total soil organic carbon and the
   contents of hydrophobic or hydrophilic groups.}},
