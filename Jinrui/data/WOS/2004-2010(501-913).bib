@article{ ISI:000241979100006,
Author = {Mentis, M. T.},
Title = {{Restoring native grassland on land disturbed by coal mining on the
   Eastern Highveld of South Africa}},
Journal = {{SOUTH AFRICAN JOURNAL OF SCIENCE}},
Year = {{2006}},
Volume = {{102}},
Number = {{5-6}},
Pages = {{193-197}},
Month = {{MAY-JUN}},
Abstract = {{This article assess process by which native grassland is restored on
   land disturbed by coal mining, mostly opencast, on the Eastern Highveld
   of South Africa. The assessment has the form of a qualitative model that
   identifies and describes the constraints on plant succession following
   disturbance. Though there is a suite of more or less interactive
   factors, key constraints concern the depletion of soil organic carbon
   and nitrogen, and depletion of the soil pools of limiting nutrients such
   as phosphorus, potassium and zinc that limit plant establishment with
   consequent soil erosion or soil crusting and hardening that render the
   reinstated landscape still more hostile to plants. There could be
   additional factors, including the development of mycorrhizal fungi and
   availability of plant propagules. There are two main ways of tackling
   the constraints - by management intervention to relax the constraints,
   and by research to improve predictive knowledge about the relevant soil
   and plant issues.}},
