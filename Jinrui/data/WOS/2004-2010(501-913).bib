@article{ ISI:000242215800038,
Author = {Smittenberg, R. H. and Eglinton, T. I. and Schouten, S. and Damste, J.
   S. Sinninghe},
Title = {{Ongoing buildup of refractory organic carbon in boreal soils during the
   Holocene}},
Journal = {{SCIENCE}},
Year = {{2006}},
Volume = {{314}},
Number = {{5803}},
Pages = {{1283-1286}},
Month = {{NOV 24}},
Abstract = {{Radiocarbon ages of vascular plant wax - derived n-alkanes preserved in
   well-dated Holocene sediments in an anoxic fjord (Saanich Inlet, Canada)
   were found to be not only substantially older than the depositional age
   but increasingly so during the Holocene. Assuming that n-alkanes serve
   as a proxy for recalcitrant terrigenous organic matter, this indicates
   that the accumulation of refractory organic carbon in soils that
   developed after the deglaciation of the American Pacific Northwest is
   ongoing and may still be far from equilibrium with mineralization and
   erosion rates.}},
