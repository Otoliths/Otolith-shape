@article{ ISI:000240746300004,
Author = {Boyer, Peter and Roberts, Neil and Baird, Douglas},
Title = {{Holocene environment and settlement on the Carsamba alluvial fan,
   south-central Turkey: Integrating geoarchaeology and archaeological
   field survey}},
