@article{ ISI:000221287700001,
Author = {Garcia-Prechac, F and Ernst, O and Siri-Prieto, G and Terra, JA},
Title = {{Integrating no-till into crop-pasture rotations in Uruguay}},
Journal = {{SOIL \& TILLAGE RESEARCH}},
