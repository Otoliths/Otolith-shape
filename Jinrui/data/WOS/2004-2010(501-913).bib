@inproceedings{ ISI:000225599600029,
Author = {Hu, YF and Liu, JY and Zhuang, DF and Wang, SQ and Yang, FT and Chen, SQ},
Editor = {{Gao, W and Shaw, DR}},
Title = {{Soil erosion effects on soil organic carbon and an assessment within
   China}},
Booktitle = {{REMOTE SENSING AND MODELING OF ECOSYSTEMS FOR SUSTAINABILITY}},
