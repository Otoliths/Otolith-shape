@article{ ISI:000225529100012,
Author = {Slaymaker, A},
Title = {{Mass balances of sediments, solutes and nutrients: The need for greater
   integration}},
Journal = {{JOURNAL OF COASTAL RESEARCH}},
Year = {{2004}},
Number = {{43}},
Pages = {{109-123}},
Month = {{SUM}},
Note = {{International Conference on Tidal Dynamics and Environment, Hangzhou,
   PEOPLES R CHINA, AUG, 2002}},
Abstract = {{The origins of sediment budgeting in both river basin and coastal
   contexts are traced back to the 1950's. Interest in global environmental
   change has spurred interest in a variety of mass balance calculations
   that permit quantitative comparison from one region to another. In river
   basin budgets the storage term is simply the difference between an
   upstream input and a downstream output over a fixed time interval; in
   coastal sediment budgets attention is focused on the residual term after
   net changes in volume of sediment within a control cell have been
   determined. Questions of scale are handled differently in river basins
   and on coasts, primarily because of the necessity to define sediment
   budget cells in coastal work. Errors and uncertainties are increasingly
