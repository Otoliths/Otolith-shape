@article{ ISI:000233849100006,
Author = {Sephton, MA and Looy, CV and Brinkhuis, H and Wignall, PB and de Leeuw,
   JW and Visscher, H},
Title = {{Catastrophic soil erosion during the end-Permian biotic crisis}},
Journal = {{GEOLOGY}},
Year = {{2005}},
Volume = {{33}},
Number = {{12}},
Pages = {{941-944}},
Month = {{DEC}},
Abstract = {{Organic geochemical analyses of sedimentary organic matter from a marine
   Permian-Triassic transition sequence in northeastern Italy reveal a
   significant influx of land-derived diagenetic products of
   polysaccharides. This unique event reflects massive soil erosion
   resulting from destruction of land vegetation due to volcanogenic
   disturbance of atmospheric chemistry. The excessive supply of soil
   materials to the oceans provides a direct link between terrestrial and
   marine ecological crises, suggesting that ecosystem collapse on land
   could have contributed to the end-Permian marine extinctions.}},
