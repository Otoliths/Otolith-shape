@article{ ISI:000243080400026,
Author = {Jankauskas, Benediktas and Slepetiene, Alvyra and Jankauskiene,
   Genovaite and Fullen, Michael A. and Booth, Colin A.},
Title = {{A comparative study of analytical methodologies to determine the soil
   organic matter content of Lithuanian Eutric Albeluvisols}},
