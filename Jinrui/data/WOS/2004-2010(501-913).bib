@article{ ISI:000225240900002,
Author = {Dougherty, WJ and Fleming, NK and Cox, JW and Chittleborough, DJ},
Title = {{Phosphorus transfer in surface runoff from intensive pasture systems at
   various scales: A review}},
Journal = {{JOURNAL OF ENVIRONMENTAL QUALITY}},
