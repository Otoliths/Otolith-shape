@article{ ISI:000229504500006,
Author = {O'Reilly, CM and Dettman, DL and Cohen, AS},
Title = {{Paleolimnological investigations of anthropogenic environmental change
   in Lake Tanganyika: VI. Geochemical indicators}},
Journal = {{JOURNAL OF PALEOLIMNOLOGY}},
Year = {{2005}},
Volume = {{34}},
Number = {{1}},
Pages = {{85-91}},
Month = {{JUL}},
Abstract = {{Geochemical signals of bulk sedimentary organic matter from three cores
   from Lake Tanganyika provided information about both internal processes
   and terrestrial inputs to the lake. Indications of land use change were
   detected in the geochemical records of the watersheds, and the timing of
   these changes was consistent with historical records of population
   demographics. While C:N ratios suggested that the distance from shore
   might be important in influencing the relative amount of allochthonous
   vs. autochthonous material, all cores were dominated by autochthonous
   organic matter. In general, nitrogen isotopes were more positive at
   disturbed sites, indicating inputs of enriched soil nitrate that was
   subsequently taken up by phytoplankton. In contrast, carbon isotopes did
   not reflect land use patterns, and a post-1950s decline in carbon
   isotope ratios found in all cores may indicate a lake-wide decrease in
   productivity. These interpretations were consistent with pollen and
   climate records.}},
