@article{ ISI:000228216900005,
Author = {Hofmann, P and Urbat, M and Hensel, A},
Title = {{Geochemical cycles recorded in Late Oligocene lake sediments of the
   Karlicher Blauton (Neuwied Basin, Germany): a response to climate?}},
Journal = {{NEUES JAHRBUCH FUR GEOLOGIE UND PALAONTOLOGIE-ABHANDLUNGEN}},
Year = {{2005}},
Volume = {{235}},
Number = {{2}},
Pages = {{265-287}},
Month = {{FEB}},
Abstract = {{Lake sediments of the Karlicher Blauton deposited during Late Oligocene
   times in the Neuwied Basin, Germany, were analyzed for major element
   concentrations (X-ray fluorescence analysis) and total organic carbon
   content (LECO analysis), complemented by organic petrology, and
   mineralogical studies. The mineralogical composition of the Karlicher
   Blanton in the core Schmidtenhohe 3 is characterized by cyclically
   fluctuating amounts of illite, muscovite, smectite, kaolinite and heavy
   minerals (rutile and zircon). The overall geochemical composition
   suggests a elastic supply to Lake Blauton from a deeply weathered
   source, probably locally exposed Devonian rocks and/or volcanogenic
   material. The cyclic depositional pattern could reflect alternating
   phases of soil development and soil erosion in the lake catchment area,
   or may be controlled by tectonic and/or volcanogenic processes.}},
