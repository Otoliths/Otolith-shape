@article{ ISI:000221394900015,
Author = {Bertol, I and Leite, D and Zoldan, WA},
Title = {{Corn crop residue decomposition and related parameters}},
Journal = {{REVISTA BRASILEIRA DE CIENCIA DO SOLO}},
