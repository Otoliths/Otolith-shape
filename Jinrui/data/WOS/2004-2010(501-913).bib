@inproceedings{ ISI:000234955700045,
Author = {Booth, CA and Fullen, MA and Jankauskas, B and Jankauskiene, G and
   Slepetiene, A},
Editor = {{Kungolos, A and Brebbia, CA and Beriatos, E}},
Title = {{The role of soil organic matter content in soil conservation and carbon
   sequestration studies: case studies from Lithuania and the UK}},
Booktitle = {{Sustainable Development and Planning II, Vols 1 and 2}},
Series = {{WIT TRANSACTIONS ON ECOLOGY AND THE ENVIRONMENT}},
Year = {{2005}},
Volume = {{84}},
Pages = {{463-473}},
Note = {{2nd International Conference on Sustainable Development and Planning,
   Bologna, ITALY, SEP 12-14, 2005}},
Organization = {{Wessex Inst Technol; WIT Transact Ecol \& Environm}},
Abstract = {{Soil organic matter (SOM) data are presented from two long-term European
   experimental research sites: (i) SOM data from a soil conservation site
   in the U.K. and (ii) SOM data from a carbon sequestration benchmarking
   site in Lithuania. Detailed SOM information, which also incorporates
   soil organic carbon (SOC), is vitally important because it plays a
   fundamental role in both soil conservation and carbon sequestration
   studies. Land management of pedogenic carbon is a recognized means of
   improving soil fertility, reducing soil erosion rates, enhancing soil
   structural stability and promoting carbon sequestration. Therefore, its
   benefits extend from local to global scales. For instance, the first
   case study illustrates the environmental benefits of changes in SOM
   content before (as bare soil) and after (converted to grassland) the
