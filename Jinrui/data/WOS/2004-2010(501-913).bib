@inproceedings{ ISI:000235640900019,
Author = {de la Cruz, MT and Casermeiro, MA and Gonzalez, GF},
Editor = {{Cano, AF and Silla, RO and Mermut, AR}},
Title = {{Influence of structural stability on water erosion processes in the
   Mediterranean area}},
Booktitle = {{SUSTAINABLE USE AND MANAGEMENT OF SOILS - ARID AND SEMIARID REGIONS}},
Series = {{ADVANCES IN GEOECOLOGY}},
Year = {{2005}},
Volume = {{36}},
Pages = {{187-196}},
Note = {{International Symposium on Sustainable Use and Management of Soils in
   Arid and Semiarid Regions, Tech Univ, Muralla del Mar Campus, Cartagena,
   SPAIN, SEP 22-26, 2002}},
Abstract = {{This paper investigates the relationship between structural stability
   and water erosion in soils developed from acidic and carbonated
   materials. Soil properties that influence the formation of aggregates
   (texture, organic carbon content, nitrogen content etc.) were examined
   in surface horizons at water erosion-affected sites in Spain. Rainfall
   simulation experiments were also performed to explore the relationship
   between structural stability, runoff and soil loss.
   Soil loss was greater in soils developed from carbonated material. In
   acidic soils, the extra clay and nitrogen favoured structural stability.
   In carbonated soils, the extra silt decreased this stability. Rainfall
   simulations showed that slope and plant cover influence runoff, and
   soils with greater structural stability are subject to less soil loss.}},
