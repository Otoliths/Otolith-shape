@inproceedings{ ISI:000242339000037,
Author = {Duttmann, Rainer and Bach, Michaela},
Editor = {{Horn, R and Fleige, H and Peth, S and Peng, XH}},
Title = {{Long-term wind erosion and its impact on soil heterogeneity in sandur
   plain landscapes in northern Germany}},
Booktitle = {{SOIL MANAGEMENT FOR SUSTAINABILITY}},
Series = {{Advances in Geoecology}},
Year = {{2006}},
Volume = {{38}},
Pages = {{309+}},
Note = {{17th Conference of the International-Soil-Tillage-Research-Organisation,
   Christian Albrechts Univ, Kiel, GERMANY, AUG 28-SEP 02, 2006}},
Organization = {{Int Soil Tillage Res Org; Inst Plant Nutr \& Soil Sci; Minist Agr,
   Environm \& Rural Landscape}},
Abstract = {{The study focuses on the estimation of wind-induced sediment transports
   on sandy soils in the sandur plain landscape of Northern Frisia
   (Germany). In order to quantify long-term wind erosion rates, the
   modeling system WEELS (Bohmer et al., 2003) was applied, using the 18 km
   2 study area of ``Goldelund{''} as an example. For a simulation period
   ranging from 1974 to 1999 the model calculates a mean annual soil loss
   of about 0.13 t {*} ha(-1) on a regional scale and taking all types of
   land use into account. Referred to the cropland only, the long-standing
   mean annual soil loss was estimated at less than 1 t {*} ha(-1), whereas
   maximum annual soil loss rates at exposed sites were calculated as 3.9 t
   {*} ha-1. Calculations performed at the 3 ha wind erosion monitoring
   site of Goldelund (BDF 4) revealed mean annual net erosion rates in a
   range between 0.1 and 0.64 t {*} ha(-1). To determine whether the
   erosion patterns derived from the WEELS model correspond to the spatial
   distribution of soil properties such as soil texture and the organic
   carbon, total nitrogen and plant available phosphorus content of the
   topsoil layer, soil samples were taken in a regular grid. The results of
   the analyses indicate that there are (1) a significant dependence
   between the in-field fetch lengths and the decline in soil organic
   matter and total nitrogen content in the A-horizon and (2) a strong
   relationship between the length of the field and the coarsening of
   texture. Because similar effects have been registered for other fields
   in this region, we assume that long-term wind erosion is a key factor
   causing in-field heterogeneity of soil property distribution and soil
   productivity in the sandur landscape of Northern Frisia.}},
