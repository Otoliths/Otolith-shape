@article{ ISI:000220487700016,
Author = {Oskarsson, H and Arnalds, O and Gudmundsson, J and Gudbergsson, G},
Title = {{Organic carbon in Icelandic Andosols: geographical variation and impact
   of erosion}},
Journal = {{CATENA}},
Year = {{2004}},
Volume = {{56}},
Number = {{1-3}},
Pages = {{225-238}},
Month = {{APR 21}},
Abstract = {{The extensive soil erosion in Iceland during the past millennium has led
   to an irregular distribution of soil organic carbon (SOC). Intact and
   fully vegetated areas typically have Andosols with large amounts of
   organic carbon, whereas eroded areas are barren deserts with little
   organic carbon. We estimate the amount of organic carbon eroded during
   the past millennium is 120500 x 10(6) t, whereas the to al organic
   carbon in Icelandic soils is 2.1 x 10(9) t. Approximately half of the
   eroded organic carbon (60-250 x 10(6) t C) has been oxidized and
   released to the atmosphere. Current ongoing losses of SOC are estimated
   to be 50-100 x 10(3) t C yr(-1). We also consider procedures for
   sequestering carbon to restore the carbon dynamics of denuded areas. (C)
   2003 Elsevier B.V All rights reserved.}},
