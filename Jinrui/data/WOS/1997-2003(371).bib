@article{ ISI:A1997XL56200006,
Author = {Kuzyakova, IF and Kuzyakov, YV},
Title = {{Effect of microrelief on the spatial variation of humus content in
   soddy-podzolic soil under long-term experimental conditions}},
Journal = {{EURASIAN SOIL SCIENCE}},
Year = {{1997}},
Volume = {{30}},
Number = {{7}},
Pages = {{727-734}},
Month = {{JUL}},
Abstract = {{The analysis of variation in the content of organic carbon in the Ap
   horizon of clay loamy soddy-podzolic soil is performed in a long-term
   held experiment with fertilizers and herbicides. It is shown that the
   C-org variation is mainly dependent on microrelief as a microerosion
   factor. A technique is proposed for calculating the local directions and
   relative intensities of temporary water-erosion flows. A classification
   is given for the microrelief forms that determine the increase and
   decrease in C-org content of soil. A conclusion is made that the natural
   variation in C-org content is continuously reproduced and is not
   eliminated by field management procedures.}},
