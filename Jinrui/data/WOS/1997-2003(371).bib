@article{ ISI:A1997XC37200008,
Author = {Garcia, C and Hernandez, T},
Title = {{Biological and biochemical indicators in derelict soils subject to
   erosion}},
Journal = {{SOIL BIOLOGY \& BIOCHEMISTRY}},
