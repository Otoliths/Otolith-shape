@inproceedings{ ISI:000178403800016,
Author = {Southorn, NJ},
Editor = {{Pagliai, M and Jones, R}},
Title = {{The soil structure component of soil quality under alternate grazing
   management strategies}},
Booktitle = {{SUSTAINABLE LAND MANAGEMENT - ENVIRONMENTAL PROTECTION: A SOIL PHYSICAL
   APPROACH}},
Series = {{ADVANCES IN GEOECOLOGY}},
Year = {{2002}},
