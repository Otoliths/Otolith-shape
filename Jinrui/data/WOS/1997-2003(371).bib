@inproceedings{ ISI:000089526200067,
Author = {Anandacoomaraswamy, A and Ananthacumaraswamy, S},
Editor = {{Robert, PC and Rust, RH and Larson, WE}},
Title = {{Precision management of soil organic carbon in tea lands of Sri Lanka}},
Booktitle = {{PROCEEDINGS OF THE FOURTH INTERNATIONAL CONFERENCE ON PRECISION
   AGRICULTURE, PTS A AND B}},
Year = {{1999}},
Pages = {{767-772}},
Note = {{4th International Conference on Precision Agriculture, ST PAUL, MN, JUL
   19-22, 1998}},
Organization = {{Univ Minnesota, Precis Agr Ctr; Univ Minnesota, Dept Soil, Water \&
   Climate; Univ Minnesota, Extens Serv; Amer Soc Agron; Crop Sci Soc Amer;
   Soil Sci Soc Amer; Soc Engn Agr, Food \& Biol Sys; Amer Soc
   Photogrammetry \& Remote Sensing}},
Abstract = {{Tea is grown as a rainfed crop in the sloping lands of Sri Lanka.
   Fertility and productivity gradients exist in these lands. The objective
   of this study is to quantify the gradients at low (>600 m AMSL), mid
   (600-900 m AMSL) and high (>900 m AMSL) elevations and under good cover
   of tea and to examine the options available to manage them precisely for
   maximum tea production. Fertility gradients were measured in terms of
   soil organic C and bulk density. Productivity was measured by yield at
   crest, shoulder, mid-slope and foot slope positions. The results suggest
   that fertility and productivity were higher at crest and foot slope and
   low at mid-slope. Further a C balance sheet was prepared for topsoil by
   taking into account all the additions and losses. The additions are leaf
   fall from tea and shade trees, tea prunings, and compost. The losses are
   due to erosions and soil respiration. Organic C requirement was
   estimated using a linear regression relating forest organic carbon and
   elevations. Using the above relation, one could work out the exact
   amount of organic matter to be added to soil for precision management.}},
