@article{ ISI:000185048000001,
Author = {Hanisch, S and Ariztegui, D and Puttmann, W},
Title = {{The biomarker record of Lake Albano, central Italy - implications for
   Holocene aquatic system response to environmental change}},
Journal = {{ORGANIC GEOCHEMISTRY}},
Year = {{2003}},
Volume = {{34}},
Number = {{9}},
Pages = {{1223-1235}},
Abstract = {{A study of lipid biomarkers in predominantly laminated, organic
   carbon-rich sediments in Lake Albano (central Italy) has been used to
   reconstruct changes in the aquatic system variations and changes in the
   take catchment area during the Holocene. Biomarkers of aquatic organic
   matter such as dinosterol, isoarborinol, tetrahymanol, diplopterol, and
   diploptene reflect two major changes in the mixing behaviour and the
   biological assemblages of Lake Albano at around 6.5 and 3.8 ky BP,
   respectively. Biomarkers for higher land plants such as friedelin,
   long-chain n-alkanes, beta-amyrenone and alpha-amyrenone display
   different distribution patterns, indicating changes in the lake's
   catchment at 6.5 and 5.0 ky BP, resulting from soil erosion and changes
   in the local vegetation. Analogously, the recognised changes in the
   water column structure and biological components are coherent with
   previous reconstructions based on independent parameters. The new data
   highlight the importance of multiproxy studies to better characterize
   palaeoenvironmental changes that are in turn the first step towards
   elucidating their trigger mechanisms (i.e. climate vs. human-induced).
   (C) 2003 Elsevier Ltd. All rights reserved.}},
