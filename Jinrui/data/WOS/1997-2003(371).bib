@article{ ISI:000089386500007,
Author = {Olivie-Lauquet, G and Allard, T and Bertaux, J and Muller, JP},
Title = {{Crystal chemistry of suspended matter in a tropical hydrosystem, Nyong
   basin (Cameroon, Africa)}},
Journal = {{CHEMICAL GEOLOGY}},
