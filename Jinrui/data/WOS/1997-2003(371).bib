@article{ ISI:000178717400004,
Author = {Hernandez-Hernandez, RM and Lopez-Hernandez, D},
Title = {{Farming modality as modifying agent of organic matter: A model for
   savanna soils in the Venezuelan central plains}},
Journal = {{INTERCIENCIA}},
