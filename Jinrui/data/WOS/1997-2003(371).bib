@article{ ISI:000076132600004,
Author = {Dumanski, J and Desjardins, RL and Tarnocai, C and Monreal, C and
   Gregorich, EG and Kirkwood, V and Campbell, CA},
Title = {{Possibilities for future carbon sequestration in Canadian agriculture in
   relation to land use changes}},
Journal = {{CLIMATIC CHANGE}},
Year = {{1998}},
Volume = {{40}},
Number = {{1}},
Pages = {{81-103}},
Month = {{SEP}},
Abstract = {{Increasing carbon sequestration in agricultural soils in Canada is
   examined as a possible strategy in slowing or stopping the current
   increase in atmospheric CO, concentrations. Estimates are provided on
   the amount of carbon that could be sequestered in soils in various
   regions in Canada by reducing summerfallow area, increased use of forage
   crops, improved erosion control, shifts from conventional to minimal and
   no-till, and more intensive use of fertilizers. The reduction of
   summerfallow by more intensive agriculture would increase the continuous
   cropland base by 8.1\% in western Canada and 6.8\% in all of Canada.
   Although increased organic carbon (OC) sequestration could be achieved
   in all agricultural regions, the greatest potential gains are in areas
   of Chernozemic soils. The best management options include reduction of
   summerfallow, conversion of fallow areas to hay or continuous cereals,
   fertilization to ensure nutrient balance, and adoption of soil
   conservation measures. The adoption of these options could sequester
   about 50-75\% of the total agricultural emissions of CO2 in Canada for
   the next 30 years. However, increased sequestration of atmospheric
   carbon in the soil is possible for only a limited time. Increased
   efforts must be made to reduce emissions if long-term mitigation is to
   be achieved.}},
