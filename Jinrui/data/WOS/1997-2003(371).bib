@article{ ISI:000071042600015,
Author = {France, RL},
Title = {{Potential for soil erosion from decreased litterfall due to riparian
   clearcutting: Implications for boreal forestry and warm- and cool-water
   fisheries}},
Journal = {{JOURNAL OF SOIL AND WATER CONSERVATION}},
Year = {{1997}},
Volume = {{52}},
Number = {{6}},
Pages = {{452-455}},
Month = {{NOV-DEC}},
Abstract = {{One of the most overt consequences of catchment clearcutting is
   increased soil erosion which can seriously affect fish stocks. Riparian
   stands around 10 northwestern Ontario lakes, clearcut 4 to 10 years
   previously were found to produce only 60\% of the litterfall of nearby
   uncut riparian forests. As a result of this decrease in protective
   ground surface cover; the retention of organic duff within deployed
   mesh-bags, was significantly reduced in clearcut riparian zones. A
   rainfall simulation experiment further suggested that the erosion of
   sandy loam can be twice as great under litterfall conditions
   representative of clearcut, compared to forested shorelines. Due to the
   potential for increased soil erosion from clearcutting, Ontario should
   consider a return to its pre-1985 policy of preventing timber harvesting
   around ``warm-{''} and ``cool-water{''} lakes that contain percid and
   esocid sportfish and often support a lucrative commercial cyprinid
