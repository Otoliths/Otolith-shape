@article{ ISI:000185066600007,
Author = {Santana, RR and Martinez, G and Macchiavelli, R and Rodriguez, JE and
   Guzman, JL},
Title = {{Potential of trees, grasses, and turf legumes for restoring eroded soils}},
Journal = {{COMMUNICATIONS IN SOIL SCIENCE AND PLANT ANALYSIS}},
Year = {{2003}},
Volume = {{34}},
Number = {{15-16}},
Pages = {{2149-2162}},
Abstract = {{The performance and development potential of seven trees, two grasses
   and five turf legumes were evaluated in three highly eroded landscapes
   of Puerto Rico. Species development during the first year of
   establishment was adequate at the Corozal site, intermediate in Vega
   Baja and poor at the Orocovis location. Tree species Albicia procera and
   Leucaena leucocephala K 636 grew particularly well at Corozal and Vega
   Baja. At the Corozal site, grasses and turf legumes were significantly
   better for reducing soil erosion than trees and bare ground. Brachiaria
   decumbens did not allow sediment losses during a highly rainy period at
   this locale; it was significantly (P < 0.01) superior to all tree
   species, one cover legume and bare ground. Soil dissolved organic carbon
   (SDOC) estimates were significantly (P < 0.01) higher for ground cover
   species than for tree species and bare ground. Soil chemical composition
   of all eroded soil sites under consideration did not explain the changes
   in performance and development of the evaluated germplasm.}},
