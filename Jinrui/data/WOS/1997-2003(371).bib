@inproceedings{ ISI:000077135800076,
Author = {Chartres, CJ and Greeves, GW},
Editor = {{Blume, HP and Eger, H and Fleischhauer, E and Hebel, A and Reij, C and Steiner, KG}},
Title = {{The impact of soil seals and crusts on soil water balance and runoff and
   their relationship with land management}},
Booktitle = {{TOWARDS SUSTAINABLE LAND USE, VOLS I \& II: FURTHERING COOPERATION
