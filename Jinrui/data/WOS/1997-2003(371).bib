@article{ ISI:A1997XL56200005,
Author = {Orlov, DS and Biryukova, ON and Ryzhova, IM},
Title = {{Dependence of humus pools on the duration of the period of soil
   biological activity}},
Journal = {{EURASIAN SOIL SCIENCE}},
Year = {{1997}},
Volume = {{30}},
Number = {{7}},
Pages = {{722-726}},
Month = {{JUL}},
Abstract = {{The equations of the dependence of the pools of organic carbon (t/ha) in
   the 0-100-cm layer on the duration (days) of the period of biological
   activity (PBA) in the major soil types are suggested. Humus pools as a
   function of PBA duration are demonstrated to increase more quickly in
   humid regions, compared to semiarid and arid ones. In the zones of
   steppes and dry steppes, humus pools can also be affected by thinned
   plant cover, soil alkalinity, erosion, and deflation.}},
