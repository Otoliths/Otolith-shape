@article{ ISI:A1991GP12100055,
Author = {MCCABE, DC and PROTZ, R and TOMLIN, AD},
Title = {{FAUNAL EFFECTS ON THE DISTRIBUTION OF GAMMA EMITTING RADIONUCLIDES IN 4
   FORESTED SOILS}},
Journal = {{WATER AIR AND SOIL POLLUTION}},
Year = {{1991}},
Volume = {{57-8}},
Pages = {{521-532}},
Month = {{AUG}},
Note = {{INTERNATIONAL CONF ON METALS IN SOILS, WATERS, PLANTS AND ANIMALS,
   ORLANDO, FL, APR 30-MAY 03, 1990}},
Organization = {{US DOE, SAVANNAH RIV OPERAT OFF}},
Abstract = {{Four forested, well drained surface horizons of soils representing the
   range of textures located in the Lake Erie Basin of Southern Ontario
   were sampled in 1 cm increments to a depth of 15 cm.  Gamma emitting
   radionuclides of natural sources, K-40, Bi-214 (U-238 daughter), and
   Tl-208 (Th-232 daughter0, are distributed as a function of texture over
   the 15 cm depth.  Increasing clay content results in increased retention
   of these isotopes.  The K-40 distribution is a result of the amount of
   organic carbon present.  Increased organic carbon results in increased
   weathering and lower K-40 values.  Differing earthworm activity results
   in distinct distributions of Cs-137, a gamma emitting radionuclide from
   bomb fallout, within the four soils.  Larger numbers and biomass of
   anecic earthworms in finer textured soils result in a linear
   distribution of Cs-137 with depth.  Endogeous species in the coarsest
   textured soil result in little incorporation of Cs-137 with depth.}},
