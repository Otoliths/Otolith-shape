@article{ ISI:A1991FF35500006,
Author = {WINNER, C},
Title = {{SUGAR-BEET PRODUCTION UNDER THE ASPECT OF ENVIRONMENTAL-PROTECTION}},
Journal = {{BERICHTE UBER LANDWIRTSCHAFT}},
Year = {{1991}},
Volume = {{69}},
Number = {{1}},
Pages = {{121-149}},
Month = {{MAR}},
Abstract = {{It has been investigated in which ways sugar beet growing is involved in
   modern farming, being intentionally adjusted to the demands of
   environmental protection.
   The following aspects have been illuminated by describing and evaluating
   present methods of beet growing in Germany:
   1.  Approaches to avoding soil erosion by new techniques of soil
   preparation and sowing
   2.  Reduction of soil compaction by wheel pressure
   3.  Ways to and success of diminishing the amount of mineral fertilizer
   applied to the beet
   4.  Overcoming incorrect application of organic manure, especially
   slurry
   5.  Ways of limiting the use of pesticides by observing the principle of
   integrated plant protection
   6.  Restricted use of herbicides as an indispensable tool of economic
   beet growing also in future
   7.  Making best use of the remaining beet leaf and of the soil adhering
   to the root after harvest (principle of recycling)
   8.  Taking full advantage of carbonation lime (sugar factory lime;
   ``Carbokalk{''}) as fertilizer and soil stabilizer.}},
