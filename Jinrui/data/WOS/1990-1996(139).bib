@article{ ISI:A1993LZ84600033,
Author = {MEYBECK, M},
Title = {{RIVERINE TRANSPORT OF ATMOSPHERIC CARBON - SOURCES, GLOBAL TYPOLOGY AND
   BUDGET}},
Journal = {{WATER AIR AND SOIL POLLUTION}},
Year = {{1993}},
Volume = {{70}},
Number = {{1-4}},
Pages = {{443-463}},
