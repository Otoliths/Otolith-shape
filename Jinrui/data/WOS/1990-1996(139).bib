@article{ ISI:A1993KY65900015,
Author = {POTTER, KN and CHICHESTER, FW},
Title = {{PHYSICAL AND CHEMICAL-PROPERTIES OF A VERTISOL WITH CONTINUOUS
   CONTROLLED-TRAFFIC, NO-TILL MANAGEMENT}},
Journal = {{TRANSACTIONS OF THE ASAE}},
Year = {{1993}},
Volume = {{36}},
Number = {{1}},
Pages = {{95-99}},
Month = {{JAN-FEB}},
Abstract = {{Conservation tillage on vertisols has not been well accepted, yet
   residue management techniques are needed to control water erosion on
   these highly-erodible soils. Surface soil properties were determined on
   a Houston black clay (Udic Pellusterts) one crop season after chisel
   plow-disk tillage, and after 6 and 10 years in a continuous
   controlled-traffic, no-till management system. Bulk density and
   penetrometer resistance below 50 mm increased with increasing time since
   primary tillage. Soil penetrometer resistance values were not in the
   root restricting range at the soil water contents at time of sampling.
   Stratification of soil nutrients, total nitrogen and phosphorus, and
   soil organic carbon increased with time since primary tillage.
   Conservation tillage appears to be sustainable on these vertisols for an
   extended period of time with the controlled-traffic, no-till practices
   used in this study.}},
