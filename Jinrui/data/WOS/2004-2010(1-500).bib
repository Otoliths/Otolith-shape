@article{ ISI:000277462300102,
Author = {Wei, Xiuguo and Yi, Weixi and Shen, Chengde and Yechieli, Yoseph and Li,
   Ningli and Ding, Ping and Wang, Ning and Liu, Kexin},
Title = {{C-14 as a tool for evaluating riverine POC sources and erosion of the
   Zhujiang (Pearl River) drainage basin, South China}},
Journal = {{NUCLEAR INSTRUMENTS \& METHODS IN PHYSICS RESEARCH SECTION B-BEAM
   INTERACTIONS WITH MATERIALS AND ATOMS}},
Year = {{2010}},
Volume = {{268}},
