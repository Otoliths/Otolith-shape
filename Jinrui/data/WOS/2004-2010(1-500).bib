@article{ ISI:000265176500014,
Author = {Abrahamson, D. A. and Causarano, H. J. and Williams, J. R. and Norfleet,
   M. L. and Franzluebbers, A. J.},
Title = {{Predicting soil organic carbon sequestration in the southeastern United
   States with EPIC and the soil conditioning index}},
Journal = {{JOURNAL OF SOIL AND WATER CONSERVATION}},
