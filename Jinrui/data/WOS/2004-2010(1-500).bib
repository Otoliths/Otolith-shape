@article{ ISI:000271436500003,
Author = {Kopacek, Jiri and Hejzlar, Josef and Kana, Jiri and Norton, Stephen A.
   and Porcal, Petr and Turek, Jan},
Title = {{Trends in aluminium export from a mountainous area to surface waters,
   from deglaciation to the recent: Effects of vegetation and soil
   development, atmospheric acidification, and nitrogen-saturation}},
Journal = {{JOURNAL OF INORGANIC BIOCHEMISTRY}},
Year = {{2009}},
Volume = {{103}},
Number = {{11}},
Pages = {{1439-1448}},
Month = {{NOV}},
Abstract = {{We reconstructed the history of terrestrial export of aluminium (Al) to
   Plesne Lake (Czech Republic) since the lake origin 12,600 year BC, and
   predicted At export for 2010-2050 on the basis of previously published
   and new data on mass budget studies, palaeolimnological data, and MAGIC
   modelling. We focused on three major At forms; ionic At (Al(i)),
   organically-bound Al (Al(o)), and particulate A1 hydroxide
   {[}Al(OH)(3)]. In early post-glacial time, Plesne Lake received high
   terrestrial export of Al, but with a minor proportion of Al(OH)(3) (4-25
   mu M), and concentrations of Al(i) and Al(o) were negligible. Since the
   forest and soil development (similar to 9900-9000 year BC), erosion has
   declined and soil organic acids increased export of Al(o) from soils.
   The terrestrial Al(o) leaching (similar to 7.5 mu M) persisted
   throughout the Holocene until the industrial period. Then, Al(i)
   concentrations continuously increased (up to 28 mu M in the mid-1980s)
   due to atmospheric acidification; the Al(i) leaching was mostly
   associated with sulphate. The proportion of Al(i) associated with
   nitrate has been increasing since the beginning of lake recovery from
   acidification after similar to 1990 due to reduction in sulphur
   deposition and nitrogen-saturation of the catchment, leading to
   persistent nitrate leaching. Currently, nitrate has become the dominant
   strong acid anion and the major Al(i) carrier. Al(o) (5.5 mu M) is
   predicted to dominate At concentrations around 2050, but the predicted
   Al(i) concentrations (similar to 4 mu M) are uncertain because of
   uncertainty associated with the future nitrate leaching and its effect
   on soils. (C) 2009 Elsevier Inc. All rights reserved.}},
