@article{ ISI:000278656400009,
Author = {Stavi, I. and Ungar, E. D. and Lavee, H. and Sarah, P.},
Title = {{Variability of soil aggregation in a hilly semi-arid rangeland}},
Journal = {{JOURNAL OF ARID ENVIRONMENTS}},
