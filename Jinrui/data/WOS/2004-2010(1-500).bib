@article{ ISI:000260578500018,
Author = {Butzer, Karl W. and Abbott, James T. and Frederick, Charles D. and
   Lehman, Paul H. and Cordova, Carlos E. and Oswald, John F.},
Title = {{Soil-geomorphology and ``wet{''} cycles in the Holocene record of
   North-Central Mexico}},
Journal = {{GEOMORPHOLOGY}},
Year = {{2008}},
Volume = {{101}},
Number = {{1-2}},
