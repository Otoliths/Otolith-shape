@article{ ISI:000262076100009,
Author = {Zhang, Shurong and Lu, X. X. and Sun, Huiquo and Han, Jingtai and
   Higgitt, Dauid Laurence},
Title = {{Geochemical characteristics and fluxes of organic carbon in a
   human-disturbed mountainous river (the Luodingjiang River) of the
   Zhujiang (Pearl River), China}},
Journal = {{SCIENCE OF THE TOTAL ENVIRONMENT}},
Year = {{2009}},
Volume = {{407}},
Number = {{2}},
Pages = {{815-825}},
Month = {{JAN 1}},
Abstract = {{This study aims to investigate the state of the riverine organic carbon
   in the Luodingjiang River under human impacts, such as reforestation,
   construction of reservoirs and in-stream damming. Seasonal and spatial
   characteristics of total suspended sediment (TSS), dissolved organic
   carbon (DOC) and particulate organic carbon (POC), as well as C/N ratios
   and the stable carbon isotopic signatures of POC (delta(13)C(POC)) were
   examined based on a one-year study (2005) in the basin-wide scale. More
   frequent sampling was conducted in the outlet of the river basin at
   Guanliang hydrological station. DOC and POC concentrations showed flush
   effects with increasing water discharge and sediment load in the
   basin-wide scale. Atomic C/N ratio of POC had a positive relationship
   with TSS in the outlet of the basin, indicating the reduced aquatic
   sources and enhanced terrestrial sources during the high flood season.
   However, the similar relationship was not observed in the basin-wide
   scale mainly due to the spatial distributions of soil organic carbon and
   TSS. delta(13)C(POC) showed obvious seasonal variations with enriched
   values in the period with high TSS concentration, reflecting the
   increased contribution from C(4) plants with enhanced soil erosion. The
   specific flux of the total organic carbon (2.30 t km(-2) year(-1)) was
   smaller than the global average level. The ratio of DOC to POC was 1.17,
   which is higher than most rivers under Asian monsoon climate regime, The
   organic carbon flux was estimated to decline with decreasing sediment
   load as a result of reforestation, reservoir construction and in-stream
   damming, which demonstrates the impacts of human disturbances on the
   global carbon cycle. (C) 2008 Elsevier B.V. All rights reserved.}},
Publisher = {{ELSEVIER SCIENCE BV}},
Address = {{PO BOX 211, 1000 AE AMSTERDAM, NETHERLANDS}},
Type = {{Article}},
Language = {{English}},
Affiliation = {{Zhang, S (Reprint Author), Natl Univ Singapore, Dept Geog, Arts Link 1, Singapore 117570, Singapore.
   Zhang, Shurong; Lu, X. X.; Higgitt, Dauid Laurence, Natl Univ Singapore, Dept Geog, Singapore 117570, Singapore.
   Sun, Huiquo; Han, Jingtai, Chinese Acad Sci, Inst Geol \& Geophys, Beijing, Peoples R China.}},
DOI = {{10.1016/j.scitotenv.2008.09.022}},
ISSN = {{0048-9697}},
