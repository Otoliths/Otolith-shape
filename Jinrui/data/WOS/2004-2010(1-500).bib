@article{ ISI:000276769800012,
Author = {Diacono, Mariangela and Montemurro, Francesco},
Title = {{Long-term effects of organic amendments on soil fertility. A review}},
Journal = {{AGRONOMY FOR SUSTAINABLE DEVELOPMENT}},
Year = {{2010}},
Volume = {{30}},
Number = {{2}},
