@article{ ISI:000272499000015,
Author = {Hakala, Kaija and Keskitalo, Marjo and Eriksson, Christian and Pitkanen,
   Timo},
Title = {{Nutrient uptake and biomass accumulation for eleven different field
   crops}},
Journal = {{AGRICULTURAL AND FOOD SCIENCE}},
Year = {{2009}},
Volume = {{18}},
Number = {{3-4, SI}},
Pages = {{366-387}},
Abstract = {{Oil hemp (Cannabis sativa L.),quinoa (Chenopodium quinoaWilld.), false
   flax (Camelina sativa (L.) Crantz), caraway (Carum carvi L.), dyer's
   woad (Isatis tinctoria L.), nettle (Urtica dioica L.), reed canary grass
   (RCG) (Phalaris arundinacea L.), buckwheat (Fagopyrum esculentum
   Moench), linseed (Linum usitatissimum L.), timothy (Phleum pratense L.)
   and barley (Hordeum vulgare L.) were grown under uniform conditions in
   pots containing well fertilised loam soil. Dry matter (DM) accumulation
   was measured repeatedly, and contents of minerals N, P, K, Ca and Mg at
   maturity. Annual crops accumulated above-ground biomass faster than
   perennials, while perennials had higher DM accumulation rates below
   ground. Seeds had high concentrations of N and P, while green biomass
   had high concentrations of K and Ca. Stems and roots had low
   concentrations of minerals. Concentrations of K and P were high in
   quinoa and caraway, and that of P in buckwheat. Hemp and nettle had high
   Ca concentrations, and quinoa had high Mg concentration. N and P were
   efficiently harvested with seed, Ca and K with the whole biomass.
   Perennials could prevent soil erosion and add carbon to the soil in the
   long term, while annuals compete better with weeds and prevent erosion
   during early growth. Nutrient balances in a field could be modified and
   nutrient leaching reduced by careful selection of the crop and
   management practices.}},
