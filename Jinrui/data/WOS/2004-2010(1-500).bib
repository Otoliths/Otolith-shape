@article{ ISI:000280030300029,
Author = {Wortmann, C. S. and Drijber, R. A. and Franti, T. G.},
Title = {{One-Time Tillage of No-Till Crop Land Five Years Post-Tillage}},
Journal = {{AGRONOMY JOURNAL}},
