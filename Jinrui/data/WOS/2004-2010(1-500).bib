@article{ ISI:000259018400008,
Author = {Llovet, J. and Josa, R. and Vallejo, V. R.},
Title = {{Thermal shock and rain effects on soil surface characteristics: A
   laboratory approach}},
Journal = {{CATENA}},
Year = {{2008}},
Volume = {{74}},
