@article{ ISI:000276763400003,
Author = {Anda, Markus and Kurnia, Undang},
Title = {{Restoring Properties of Artificially Degraded Ultisols and Oxisols and
   the Effect on Crop Yields under Tropical Conditions}},
Journal = {{COMMUNICATIONS IN SOIL SCIENCE AND PLANT ANALYSIS}},
