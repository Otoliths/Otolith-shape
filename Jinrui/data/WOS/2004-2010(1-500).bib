@article{ ISI:000268776400009,
Author = {Ozdemir, Nutullah and Yakupoglu, Tugrul and Dengiz, Orhan},
Title = {{The effects of bio-solid and tea waste application into different levels
   of eroded soil on N, P and K concentrations}},
Journal = {{ENVIRONMENTAL MONITORING AND ASSESSMENT}},
