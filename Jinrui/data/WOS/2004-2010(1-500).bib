@article{ ISI:000258153200009,
Author = {Smith, Stephen V. and Ibarra-Obando, Silvia E. and Diaz-Castaneda,
   Victoria and Aranda-Manteca, Francisco Javier and Carriquiry, Jose D.
   and Popp, Brian N. and Gonzalez-Yajimovich, Oscar},
Title = {{Sediment organic carbon in Todos Santos Bay, Baja California, Mexico}},
Journal = {{ESTUARIES AND COASTS}},
Year = {{2008}},
Volume = {{31}},
Number = {{4}},
Pages = {{719-727}},
Month = {{SEP}},
Abstract = {{Sediment grain size and organic carbon (OC) data collected over the past
   50 years, together with delta(13)C values of OC in recently collected
   samples, were analyzed to improve understanding of sediment OC
   distribution and abundance in Todos Santos Bay. Sediments in the
   submarine canyon at the mouth of the bay and in quiet-water locations
   along the shore are fine grained, high in OC, and have generally low
   delta(13)C values; sediments in high-energy environments are low in OC
   and have high delta(13)C values. A bivariate isotopic mixing model
   indicates that none of the sediments contain > 50\% terrigenous OC
   (average similar to 30\%), and that the terrigenous OC content of the
   sediments is a small proportion of the OC content of local soils.
   Sediment OC composition is apparently controlled by energy-related
   sorting and deposition, oxidation of much of the original terrigenous
   OC, and replacement of some terrigenous OC by marine OC.}},
