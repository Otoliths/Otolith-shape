@article{ ISI:000265924500006,
Author = {Ding Ping and Shen ChengDe and Wang Ning and Yi WeiXi and Liu KeXin and
   Ding XingFang and Fu DongPo},
Title = {{Carbon isotopic composition and its implications on paleoclimate of the
   underground ancient forest ecosystem in Sihui, Guangdong}},
Journal = {{SCIENCE IN CHINA SERIES D-EARTH SCIENCES}},
Year = {{2009}},
Volume = {{52}},
Number = {{5}},
Pages = {{638-646}},
Month = {{MAY}},
Abstract = {{We present the carbon isotopic composition of the total organic carbon
   (TOC) and fine roots in the sedimentary profile from the underground
   ancient forest in Sihui to study the climatic and environmental changes
   from 4.5 ka BP to 0.6 ka BP. Results show that C-3 plant was the main
   vegetation from 4.5 ka BP to 0.6 ka BP in this region. The ancient
   forest began to develop in the wetland at around 4 ka BP and disappeared
   together with the wetland at about 3.0 ka BP, implying that the climate
   had changed greatly at around 3.0 ka BP. As indicated by the simulation
   results, the content of atmospheric CO2 increased slightly during 3.5 ka
   BP to 3.0 ka BP, implying climate warming during that period. The
   interval of radiocarbon age between 3.0 ka BP to 1.2 ka BP was possibly
   caused by the strong erosion when the block was lifted in the
   neotectonic movement. From 1.2 ka BP to 0.6 ka BP, the region remained
   in terrestrial sedimentary environment, and the surface plant biomass
   declined gradually. Drought caused by the climate change was the likely
   cause for the disappearance of the ancient forest. South transition of
   Intertropical Convergence Zone (ITCZ) was probably the main mechanism
   for the climate change.}},
