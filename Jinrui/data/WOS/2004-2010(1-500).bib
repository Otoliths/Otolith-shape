@article{ ISI:000260366500006,
Author = {Shotbolt, L. A. and Rothwell, J. J. and Lawlor, A. J.},
Title = {{A mass balance approach to quantifying Pb storage and fluxes in an
   upland catchment of the Peak District, north-central England}},
Journal = {{EARTH SURFACE PROCESSES AND LANDFORMS}},
Year = {{2008}},
Volume = {{33}},
Number = {{11}},
Pages = {{1721-1741}},
Month = {{OCT}},
Abstract = {{A mass balance model of the main Pb stores and fluxes for a typical
   organic-rich upland catchment in the Peak District, UK, has been
   produced. The model, based on the Howden reservoir catchment, reveals
   that the majority, of Pb in (lie catchment is stored within the soil
   (approximately 8-63 t km(-2)). Soil Pb levels are extremely high and
   call only he explained as the result of centuries of atmospheric Pb
   deposition from sui-rounding urban-industrial conurbations, and mining
   and smelting activity within the Peak District National Park. The
   atmospheric Pb flux onto the Howden catchment is approximately 107 kg
   a(-1). The aquatic Ph flux is estimated at between 29-9 and 71.7 kg
   a(-1); thus, at present, catchnient soils are acting as a sink for Pb
   pollution. The Howden reservoir acts as a secondary store for Pb eroded
   and leached from catchnient soils, with approximately 80\% re-deposited
   in its sediments. It is estimated (hat 2-3\% of the catchnient, soil Pb
   pool has been retained in (he reservoir sediments over its 91 year
   lifespan.
