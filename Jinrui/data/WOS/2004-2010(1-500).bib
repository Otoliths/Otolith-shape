@article{ ISI:000285574100005,
Author = {Van Hemelryck, H. and Fiener, P. and Van Oost, K. and Govers, G. and
   Merckx, R.},
Title = {{The effect of soil redistribution on soil organic carbon: an
   experimental study}},
