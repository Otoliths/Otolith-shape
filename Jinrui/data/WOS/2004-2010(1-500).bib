@article{ ISI:000262812700012,
Author = {Li, Yunlin and Cui, Jianyuan and Zhang, Tonghui and Okuro, Toshiya and
   Drake, Sam},
Title = {{Effectiveness of sand-fixing measures on desert land restoration in
   Kerqin Sandy Land, northern China}},
