@article{ ISI:000293038700022,
Author = {Powers, S. E. and Ascough, II, J. C. and Nelson, R. G. and Larocque, G.
   R.},
Title = {{Modeling water and soil quality environmental impacts associated with
   bioenergy crop production and biomass removal in the Midwest USA}},
