@article{ ISI:000291168900014,
Author = {Kocman, David and Kanduc, Tjasa and Ogrinc, Nives and Horvat, Milena},
Title = {{Distribution and partitioning of mercury in a river catchment impacted
   by former mercury mining activity}},
